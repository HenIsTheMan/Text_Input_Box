            <kind>1</kind>
            <string>if(canWrite &amp;&amp; keyboard_check_pressed(vk_anykey)){
    if(string_length(chr(keyboard_lastkey)) = 1){
        keyboardString = string_insert(keyboard_lastchar, keyboardString, caret + 1);
        if(!keyboard_check_pressed(vk_backspace)){
            caret++;
        }
    }
    alarm[1] = 20;
    state = 1;
}

if(!keyboard_check(vk_backspace)){
    alarm[0] = 7;
}
if(keyboard_check_pressed(vk_backspace) || (keyboard_check(vk_backspace) &amp;&amp; alarm[0] == -1)){
    if(caret &gt; 0){
        keyboardString = string_delete(keyboardString, caret, 1);
        caret--;
    }
}

if(keyboard_check_pressed(vk_home)){
    caret = 0;
}
if(keyboard_check_pressed(vk_end)){
    caret = string_length(keyboardString);
}
if(keyboard_check_pressed(vk_left) &amp;&amp; caret &gt; 0){
    caret--;
}
if(keyboard_check_pressed(vk_right) &amp;&amp; caret &lt; string_length(keyboardString)){
    caret++;
}

if(mouse_check_button_pressed(mb_left)){
    if(position_meeting(mouse_x,mouse_y,self)){
        canWrite = 1;
        alarm[1] = 20;
        state = 1;
    } else{
        canWrite = 0;
    }
    for(i = 0; i &lt; string_length(keyboardString); i++){
        width[i] = string_width(string_copy(keyboardString,i+1,1));
    }
    index = 0;
    check = 0;
    while(check &lt; mouse_x - xx){
        if(index &lt; string_length(keyboardString)){
            check += width[index];
            index++;
        } else{
            break;
        }
    }
    if((mouse_x - xx) - string_width(string_copy(keyboardString,1,index - 1)) &lt; string_width(string_copy(keyboardString,1,index)) - (mouse_x - xx)){
        caret = index - 1;
    } else{
        caret = index;
    }
}

if(keyboardString != ""){
    stringPart = string_copy(keyboardString,1,caret);
} else{
    stringPart = "";
}

if(keyboard_check_pressed(vk_enter) &amp;&amp; keyboardString != ""){
    if(room_exists(asset_get_index(string_upper(keyboardString)))){
        room_goto(asset_get_index(string_upper(keyboardString)));
    } else{
        room_goto(room_duplicate(WORLD));
    }
}
</string>
